fn bubble_sort(arr: [i32;10], compare_function: fn(i32, i32) -> i32) -> () {
  let n: i32 = 10;
  let i: i32 = 0;
  loop n {
      i = i + 1;
      let j: i32 = 0;
      loop n - i - 1 {
          j = j + 1;
          if compare_function(arr[j], arr[j + 1]) > 0 {
              let temp: i32 = arr[j];
              arr[j] = arr[j + 1];
              arr[j + 1] = temp;
          }
      }
  }
}