{
  "name": "tree-sitter-inference",
  "version": "0.0.23",
  "description": "Inference grammar for tree-sitter",
  "main": "bindings/node",
  "types": "bindings/node",
  "author": "Georgii Plotnikov <accembler@gmail.com>",
  "license": "MIT",
  "contributors": [],
  "maintainers": [],
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "inference"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Inferara/tree-sitter-inference.git"
  },
  "bugs": {
    "url": "https://github.com/Inferara/tree-sitter-inference/issues"
  },
  "homepage": "https://github.com/Inferara/tree-sitter-inference#readme",
  "scripts": {
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip",
    "lint": "eslint grammar.js",
    "bump_version": "./bump_version.sh",
    "b": "tree-sitter generate",
    "t": "tree-sitter test",
    "bt": "npm run b && npm run t",
    "d": "tree-sitter parse debug.inf"
  },
  "dependencies": {
    "node-addon-api": "8.3.0",
    "node-gyp-build": "4.8.4"
  },
  "peerDependencies": {
    "tree-sitter": "0.22.1"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "eslint": "9.17.0",
    "eslint-config-google": "^0.14.0",
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "0.24.5"
  },
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ]
}